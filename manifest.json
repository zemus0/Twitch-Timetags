{
    "name": "Twitch Timetags",
    "description": "Timestamps with messages for twitch streams",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs"
    ],
    "action": {
        "default_title": "Timetag for Twitch",
        "default_icon": {
            "16": "/images/16.png",
            "32": "/images/32.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
          }
        },
    "icons": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    },
    "background": {
        "service_worker":"background.js"
    }
}